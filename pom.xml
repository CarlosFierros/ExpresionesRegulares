import java.util.regex.Pattern;
        import java.util.regex.Matcher;

        public class EmailValidator {
        public static void main(String[] args) {
        // Expresi칩n regular para validar correos de @unison.mx o @uson.mx
        String regex = "^[a-zA-Z0-9._%+-]+@(unison|uson)\\.mx$";
        Pattern pattern = Pattern.compile(regex);

        // Ejemplos de correos a validar
        String[] emails = {
        "alumno@unison.mx",
        "profesor@uson.mx",
        "nombre.apellido@unison.mx",
        "nombre123@uson.mx",
        "incorrecto@otrodominio.com",
        "malformato@unisoncom"
        };

        // Validaci칩n de los correos
        for (String email : emails) {
        Matcher matcher = pattern.matcher(email);
        if (matcher.matches()) {
        System.out.println(email + " es v치lido.");
        } else {
        System.out.println(email + " no es v치lido.");
        }
        }
        }
        }